<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.StackPane?>
<?import org.controlsfx.control.GridView?>

<SplitPane dividerPositions="0.55" maxHeight="200" maxWidth="-Infinity" minHeight="100" minWidth="500" orientation="VERTICAL" prefHeight="400.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.TeleCommandControl">
   <items>
      <StackPane prefHeight="150.0" prefWidth="200.0">
         <children>
            <Label text="Tele-Command:" StackPane.alignment="TOP_LEFT" />
            <Label text="predefined Commands: u can create them in the config." StackPane.alignment="BOTTOM_LEFT" />
            <Button mnemonicParsing="false" onMouseClicked="#btnSendCommand" text="Send" StackPane.alignment="CENTER_RIGHT" />
            <TextField fx:id="inputField">
               <StackPane.margin>
                  <Insets right="80.0" />
               </StackPane.margin></TextField>
            <ChoiceBox fx:id="commandCoiceBox" prefWidth="150.0" StackPane.alignment="TOP_RIGHT" />
         </children>
         <padding>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </padding>
      </StackPane>

      <GridView fx:id="gridView">
      </GridView>
   </items>
</SplitPane>
